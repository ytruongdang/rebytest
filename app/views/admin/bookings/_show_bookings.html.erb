<div class="bookings">
  <div class="col-md-12">
    <table class="table table-hover table-responsive table-striped
      table-bordered">
      <thead>
        <tr>
          <th><%= t ".contact_name" %></th>
          <th><%= t ".contact_phone" %></th>
          <th><%= t ".contact_address" %></th>
          <th><%= t ".num_tourist" %></th>
          <th><%= t ".total_price" %></th>
          <th><%= t ".status" %></th>
          <th><%= t ".created_at" %></th>
        </tr>
      </thead>
      <tbody>
        <%= render @supports.load_bookings %>
      </tbody>
    </table>
  </div>
  <div class="col-md-12 text-center">
    <%= will_paginate @supports.load_bookings %>
  </div>
</div>
